# Google Gemini API Key
# Obtén tu API Key en: https://ai.google.dev/
VITE_GEMINI_API_KEY=tu_api_key_aqui

# Configuración de Caché (opcional)
# Tiempo de expiración de caché en horas (default: 24)
VITE_CACHE_EXPIRATION_HOURS=24

# Tamaño máximo de historial (default: 10)
VITE_MAX_HISTORY_SIZE=10

# Habilitar modo debug (default: false)
VITE_DEBUG=false
